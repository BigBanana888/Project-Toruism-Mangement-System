<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.api.mapper.TSubMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.api.pojo.TSub">
        <id column="id" property="id" />
        <result column="uid" property="uid" />
        <result column="remark" property="remark" />
        <result column="aid" property="aid" />
        <result column="started_time" property="startedTime" />
        <result column="status" property="status" />
        <result column="created_time" property="createdTime" />
        <result column="modified_time" property="modifiedTime" />
        <result column="created_id" property="createdId" />
        <result column="modified_id" property="modifiedId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, uid, remark, aid, started_time, status, created_time, modified_time, created_id, modified_id
    </sql>

</mapper>
